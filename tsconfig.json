{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "typeRoots": [ "./node_modules/@types", "./@types" ],
    "types": [ "styled-components-react-native" ],
    "baseUrl": "./",
    "paths": {
      "@assets/*": [ "./src/assets/*" ],
      "@components/*": [ "./src/components/*" ],
      "@routes/*": [ "./src/routes/*" ],
      "@screens/*": [ "./src/screens/*" ],
      "@storage/*": [ "./src/storage/*" ],
      "@theme/*": [ "./src/theme/*" ],
      "@utils/*": [ "./src/utils/*" ],
    }
  }
}
